{
	"name": "embedme",
  "version": "0.1.0",
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "debug": {
      "all": false
    },
    "exportInterfaces": true
  },
	"sources": [
		{
			"dir": "src",
			"subdirs": true
		},
		{
			"dir": "test",
			"dev": true
		}
	],
	"package-specs": {
		"module": "commonjs",
		"in-source": true
	},
	"suffix": ".bs.js",
	"bs-dependencies": [ "@ava/bucklescript" ],
	"warnings": {
		"error": "+101"
	},
	"namespace": true,
	"refmt": 3
}
